<extend name="Layout:layout"/>
<block name="main-content">
            	<div class="mws-panel grid_8">
                	<div class="mws-panel-header">
                    	<span>添加品牌</span>
                    </div>
                    <div class="mws-panel-body no-padding">
                    	<form class="mws-form" action="__CONTROLLER__/insert" method="post" enctype="multipart/form-data">
                    		<div class="mws-form-inline">
                    			<div class="mws-form-row">
                    				<label class="mws-form-label">品牌</label>
                    				<div class="mws-form-item">
                    					<input type="text" class="small" name='brand'>
                    				</div>
                    			</div>
								<div class="mws-form-row">
                    				<label class="mws-form-label">车系</label>
                    				<div class="mws-form-item">
                    					<input type="text" class="small" name='series'>
                    				</div>
                    			</div>
								<div class="mws-form-row">
                    				<label class="mws-form-label">车型</label>
                    				<div class="mws-form-item">
                    					<input type="text" class="small" name='models'>
                    				</div>
                    			</div>
                       		</div>
                    		<div class="mws-button-row">
                    			<input type="submit" value="添加" class="btn btn-danger">
                    			<input type="reset" value="重置" class="btn ">
                    		</div>
                    	</form>
                    </div>    	
                </div>
</block>