<extend name="Layout:layout" />
<block name="main-content">
    <div class="row">
        <div class="col-sm-12">
            <section class="panel">
                <header class="panel-heading">
                    所有用户
                    <span class="tools pull-right">
                        <a href="javascript:;" class="fa fa-chevron-down"></a>
                    </span>
                </header>
                <div class="panel-body">
                    <div class="adv-table">
                        <table  class="display table table-bordered table-striped" id="dynamic-table">
                            <thead>
                                <tr>
                                    <th>编号</th>
                                    <th>品牌</th>
                                    <th>车系</th>
                                    <th>车型</th>
                                    <th>套餐</th>
                                    <th>最低价</th>
                                    <th>最高价</th>
                                    <th>操作</th>
                                </tr>
                            </thead>
                            <tbody>
                                <foreach name="users" item="users">
                                    <tr class="gradeX">
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                    </tr>
                                </foreach>
                    		</tbody>
		
                        </table>
                    </div>
                </div>
            </section>
        </div>
    </div>
</block>